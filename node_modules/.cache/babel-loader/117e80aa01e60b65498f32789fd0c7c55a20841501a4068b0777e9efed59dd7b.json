folio</h1>\n    \n    <h1 class=\"display-6 text-primary\">{{ name }}</h1>\n    <img src=\"./assets/addeen.png\" alt=\"\">\n    <p class=\"text-danger text-opacity-50\">{{ des }}</p>\n    <p class=\"text-danger\">{{ hobbies }}</p>\n    \n    <p>ผลการเรียน</p>\n    <button class=\"btn btn-outline-primary btn-sm mx-1\" @click=\"toggleAdd()\">เพิ่มผลการเรียน</button>\n    <button class=\"btn btn-outline-secondary btn-sm mx-1\" @click=\"toggleList()\">แสดงผลการเรียน</button>\n    \n    <div class=\"m-2\" v-if=\"showSubAdd\">\n      <SubAdd/> \n    </div>\n    <br><br>\n    <div class=\"m-2\" v-if=\"showSubList\">\n      <SubList :students=\"students\" />\n    </div>\n    <div class=\"m-2\" v-if=\"showSubEdit\">\n      <SubEdit :stdId=\"someIdHere\" />\n    </div>\n    \n  </div>\n</template>\n\n<script>\nimport SubAdd from './components/SubAdd'\nimport SubList from './components/SubList'\n\nexport default {\n  name: 'App',\n  components: {\n    SubList,\n    SubAdd,\n  },\n  data() {\n    return {\n      name: \"\",\n      major: \"\",\n      des: \"\",\n      hobbies : \"\",\n      image:\"\",\n      showSubList: false,\n      showSubAdd: false,\n      showSubEdit: false,\n      students: [],\n      someIdHere: 123\n    }\n  },\n  mounted() {\n    fetch('http://localhost:3000/data_port')\n        .then(res => res.json())\n        .then(data => {\n          if (Array.isArray(data) && data.length > 0) {\n            const student = data[0];\n            this.name = student.name || 'ไม่พบข้อมูล';\n            this.des = student.des || 'ไม่พบข้อมูล';\n            this.hobbies = student.hobbies || 'ไม่พบข้อมูล';\n            this.image = student.image || 'ไม่พบข้อมูล';\n            this.students = data;\n          } else {\n            this.name = 'ไม่พบข้อมูล';\n            this.major = 'ไม่พบข้อมูล';\n            this.des = 'ไม่พบข้อมูล';\n          }\n        })\n        .catch(err => console.log(err.message))\n  },\n  methods: {\n    toggleList() {\n      this.showSubList = !this.showSubList;\n      this.showSubAdd = false;\n      this.showSubEdit = false;\n    },\n    toggleAdd() {\n      this.showSubAdd = !this.showSubAdd;\n      this.showSubList = false;\n      this.showSubEdit = false;\n    },\n    toggleEdit() {\n      this.showSubEdit = !this.showSubEdit;\n      this.showSubList = false;\n      this.showSubAdd = false;\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>"],"mappings":"AA4BA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,OAAM,MAAO,sBAAqB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF,OAAO;IACPD;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,IAAI,EAAE,EAAE;MACRG,KAAK,EAAE,EAAE;MACTC,GAAG,EAAE,EAAE;MACPC,OAAM,EAAI,EAAE;MACZC,KAAK,EAAC,EAAE;MACRC,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE,KAAK;MACjBC,WAAW,EAAE,KAAK;MAClBC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,KAAK,CAAC,iCAAiC,EAClCC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACtBF,IAAI,CAACZ,IAAG,IAAK;MACZ,IAAIe,KAAK,CAACC,OAAO,CAAChB,IAAI,KAAKA,IAAI,CAACiB,MAAK,GAAI,CAAC,EAAE;QAC1C,MAAMC,OAAM,GAAIlB,IAAI,CAAC,CAAC,CAAC;QACvB,IAAI,CAACF,IAAG,GAAIoB,OAAO,CAACpB,IAAG,IAAK,aAAa;QACzC,IAAI,CAACI,GAAE,GAAIgB,OAAO,CAAChB,GAAE,IAAK,aAAa;QACvC,IAAI,CAACC,OAAM,GAAIe,OAAO,CAACf,OAAM,IAAK,aAAa;QAC/C,IAAI,CAACC,KAAI,GAAIc,OAAO,CAACd,KAAI,IAAK,aAAa;QAC3C,IAAI,CAACI,QAAO,GAAIR,IAAI;MACtB,OAAO;QACL,IAAI,CAACF,IAAG,GAAI,aAAa;QACzB,IAAI,CAACG,KAAI,GAAI,aAAa;QAC1B,IAAI,CAACC,GAAE,GAAI,aAAa;MAC1B;IACF,CAAC,EACAiB,KAAK,CAACC,GAAE,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,OAAO,CAAC;EAC5C,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACpB,WAAU,GAAI,CAAC,IAAI,CAACA,WAAW;MACpC,IAAI,CAACC,UAAS,GAAI,KAAK;MACvB,IAAI,CAACC,WAAU,GAAI,KAAK;IAC1B,CAAC;IACDmB,SAASA,CAAA,EAAG;MACV,IAAI,CAACpB,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;MAClC,IAAI,CAACD,WAAU,GAAI,KAAK;MACxB,IAAI