{"ast":null,"code":"import StdAdd from './components/StdAdd';\nimport StdList from './components/StdList';\nexport default {\n  name: 'App',\n  components: {\n    StdList,\n    StdAdd\n  },\n  data() {\n    return {\n      name: \"\",\n      major: \"\",\n      des: \"\",\n      hobbies: \"\",\n      showStdList: false,\n      showStdAdd: false,\n      students: [] // เพิ่ม property สำหรับเก็บข้อมูลนิสิต\n    };\n  },\n  mounted() {\n    fetch('http://localhost:3000/data_port').then(res => res.json()).then(data => {\n      if (Array.isArray(data) && data.length > 0) {\n        const student = data[0];\n        this.name = student.name || 'ไม่พบข้อมูล';\n        this.major = student.major || 'ไม่พบข้อมูล';\n        this.des = student.des || 'ไม่พบข้อมูล';\n        this.students = data;\n      } else {\n        this.name = 'ไม่พบข้อมูล';\n        this.major = 'ไม่พบข้อมูล';\n        this.des = 'ไม่พบข้อมูล';\n      }\n    }).catch(err => console.log(err.message));\n  },\n  methods: {\n    toggleList() {\n      this.showStdList = !this.showStdList;\n      this.showStdAdd = false;\n    },\n    toggleAdd() {\n      this.showStdAdd = !this.showStdAdd;\n      this.showStdList = false;\n    }\n  }\n};","map":{"version":3,"names":["StdAdd","StdList","name","components","data","major","des","hobbies","showStdList","showStdAdd","students","mounted","fetch","then","res","json","Array","isArray","length","student","catch","err","console","log","message","methods","toggleList","toggleAdd"],"sources":["C:\\Users\\ADDEEN\\Downloads\\scistd\\scistd\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <h1 class=\"display-5 text-primary\">Portfolio</h1>\n    <h1 class=\"display-6 text-primary\">{{ name }}</h1>\n    <h3 class=\"text-danger\"> {{ major }}</h3>\n    <p class=\"text-danger text-opacity-50\">{{ des }}</p>\n    <p class=\"text-danger\">{{ hobbies }}</p>\n    <p>ผลการเรียน</p>\n    <button class=\"btn btn-outline-primary btn-sm mx-1\" @click=\"toggleAdd()\">เพิ่มผลการเรียน</button>\n    <button class=\"btn btn-outline-secondary btn-sm mx-1\" @click=\"toggleList()\">แสดงผลการเรียน</button>\n    <div class=\"m-2\" v-if=\"showStdAdd\">\n      <StdAdd/> \n    </div>\n    <div class=\"m-2\" v-if=\"showStdList\">\n      <StdList :students=\"students\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport StdAdd from './components/StdAdd'\nimport StdList from './components/StdList'\n\nexport default {\n  name: 'App',\n  components: {\n    StdList,\n    StdAdd\n  },\n  data() {\n    return {\n      name: \"\",\n      major: \"\",\n      des: \"\",\n      hobbies : \"\",\n      showStdList: false,\n      showStdAdd: false,\n      students: []  // เพิ่ม property สำหรับเก็บข้อมูลนิสิต\n    }\n  },\n  mounted() {\n    fetch('http://localhost:3000/data_port')\n        .then(res => res.json())\n        .then(data => {\n          if (Array.isArray(data) && data.length > 0) {\n            const student = data[0];\n            this.name = student.name || 'ไม่พบข้อมูล';\n            this.major = student.major || 'ไม่พบข้อมูล';\n            this.des = student.des || 'ไม่พบข้อมูล';\n            this.students = data;\n          } else {\n            this.name = 'ไม่พบข้อมูล';\n            this.major = 'ไม่พบข้อมูล';\n            this.des = 'ไม่พบข้อมูล';\n          }\n        })\n        .catch(err => console.log(err.message))\n  },\n  methods: {\n    toggleList() {\n      this.showStdList = !this.showStdList;\n      this.showStdAdd = false;\n    },\n    toggleAdd() {\n      this.showStdAdd = !this.showStdAdd;\n      this.showStdList = false;\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"mappings":"AAoBA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,OAAM,MAAO,sBAAqB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF,OAAO;IACPD;EACF,CAAC;EACDI,I