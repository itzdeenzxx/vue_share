{"ast":null,"code":"import SubDetail from './SubDetail.vue';\nexport default {\n  name: 'SubList',\n  components: {\n    SubDetail\n  },\n  data() {\n    return {\n      subject: [],\n      isEdit: false,\n      EditId: ''\n    };\n  },\n  mounted() {\n    fetch('http://localhost:3000/subject').then(res => res.json()).then(data => this.subject = data).catch(err => console.log(err.message));\n  },\n  methods: {\n    setShow(theId) {\n      theId.isShow = !theId.isShow;\n    },\n    showEdit(theStdId) {\n      this.EditId = theStdId;\n      this.isEdit = true;\n    },\n    reload() {\n      this.$parent.showSubList = !this.$parent.showSubList;\n    }\n  }\n};","map":{"version":3,"names":["SubDetail","name","components","data","subject","isEdit","EditId","mounted","fetch","then","res","json","catch","err","console","log","message","methods","setShow","theId","isShow","showEdit","theStdId","reload","$parent","showSubList"],"sources":["/Users/pim/Documents/Addeen/vue2/vue_share/src/components/SubList.vue"],"sourcesContent":["<template>\n    <div>\n        <h2>ผลการเรียน</h2>\n        <ul class=\"list-group\">\n            <li v-for=\"(std, id) in subject\" :key=\"id\" class=\"list-group-item\" id=\"list\">\n                \n                <a href=\"#list\" @click=\"setShow(std)\">\n                    {{ std.name }}\n                </a>\n                <div v-if=\"std.isShow\">\n                    <SubDetail :stdId=\"std.id\" @edit=\"showEdit\" @delete=\"reload()\"/>\n                </div>\n            </li>\n            <!-- <div v-if=\"std.isShow\">\n                <SubDetail :stdId=\"std.id\" />\n            </div> -->\n            <div v-if=\"isEdit\">\n                {{ EditId }}\n            </div>\n        </ul>\n        <br><br>\n    </div>\n</template>\n\n<script>\nimport SubDetail from './SubDetail.vue';\nexport default {\n    name: 'SubList',\n    components:{\n        SubDetail\n    },\n    data() {\n        return {\n            subject: [],\n       