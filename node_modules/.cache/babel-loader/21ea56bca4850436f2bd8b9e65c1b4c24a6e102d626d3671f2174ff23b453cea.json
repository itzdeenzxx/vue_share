e.log(err.message))\n  },\n  methods: {\n    toggleList() {\n      this.showSubList = !this.showSubList;\n      this.showSubAdd = false;\n      this.showSubEdit = false;\n    },\n    toggleAdd() {\n      this.showSubAdd = !this.showSubAdd;\n      this.showSubList = false;\n      this.showSubEdit = false;\n    },\n    toggleEdit() {\n      this.showSubEdit = !this.showSubEdit;\n      this.showSubList = false;\n      this.showSubAdd = false;\n    }\n  }\n}\n</script>\n\n<style>\nbody {\n  background-image: url(./assets/bg.jpg);\n}\n.main {\n  font-family: \"Prompt\", sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 40px;\n}\n.banner-profile {\n\n}\n.image-me {\n  border-radius: 16px;\n  /* box-shadow: 0px -2px 6px 5px rgba(0, 0, 0, 0.1); */\n  }\n.description-profile {\n  /* margin: 20px; */\n  margin-left: 40px;\n  margin-right: 40px;\n  font-size: 30px;\n}\n.hobbies-me\n</style>\n"],"mappings":"AAiCA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,OAAM,MAAO,sBAAqB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF,OAAO;IACPD;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,IAAI,EAAE,EAAE;MACRG,GAAG,EAAE,EAAE;MACPC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAC,EAAE;MACRC,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE,KAAK;MACjBC,WAAW,EAAE,KAAK;MAClBC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,KAAK,CAAC,iCAAiC,EAClCC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACtBF,IAAI,CAACX,IAAG,IAAK;MACZ,IAAIc,KAAK,CAACC,OAAO,CAACf,IAAI,KAAKA,IAAI,CAACgB,MAAK,GAAI,CAAC,EAAE;QAC1C,MAAMC,OAAM,GAAIjB,IAAI,CAAC,CAAC,CAAC;QACvB,IAAI,CAACF,IAAG,GAAImB,OAAO,CAACnB,IAAG,IAAK,aAAa;QACzC,IAAI,CAACG,GAAE,GAAIgB,OAAO,CAAChB,GAAE,IAAK,aAAa;QACvC,IAAI,CAACC,OAAM,GAAIe,OAAO,CAACf,OAAM,IAAK,aAAa;QAC/C,IAAI,CAACC,KAAI,GAAIc,OAAO,CAACd,KAAI,IAAK,aAAa;QAC3C,IAAI,CAACI,QAAO,GAAIP,IAAI;MACtB,OAAO;QACL,IAAI,CAACF,IAAG,GAAI,aAAa;QACzB,IAAI,CAACoB,KAAI,GAAI,aAAa;QAC1B,IAAI,CAACjB,GAAE,GAAI,aAAa;MAC1B;IACF,CAAC,E